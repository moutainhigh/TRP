<?xml version="1.0" encoding="UTF-8"?>
<!-- [syncWithModel=true] -->
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping >

<class 
  name="com.fdays.tsms.airticket.Flight"
	table="flight"
>

<meta attribute="extends">org.apache.struts.action.ActionForm implements Cloneable</meta>
<meta attribute="class-code"><![CDATA[
  public Object clone()
  {
    Object o = null;
    try
    {
      o = super.clone();
    }
    catch (CloneNotSupportedException e)
    {
      e.printStackTrace();
    }
    return o;
}

private String thisAction="";
 public String getThisAction()
 {
     return thisAction;
 }


public void setThisAction(String thisAction)
 {
     this.thisAction=thisAction;
 }

private int index=0;
 public int getIndex()
 {
     return index;
 }


public void setIndex(int index)
 {
     this.index=index;
 }
 




]]></meta>


    <id
        name="id"
        type="long"
        column="id"
    >
        <generator class="sequence" >
          <param  name="sequence">seq_flight</param>
          </generator>
    </id>

    <property
        name="flightCode"
        column="flight_code"
        type="string"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="startPoint"
        column="start_point"
        type="string"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="endPoint"
        column="end_point"
        type="string"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="boardingTime"
        column="boarding_time"
        type="java.sql.Timestamp"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="flightClass"
        column="flight_class"
        type="string"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="discount"
        column="discount"
        type="string"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="status"
        column="status"
        type="java.lang.Long"
        not-null="false"
        insert="true" update="true"
    >
	</property>



    <many-to-one
        name="airticketOrder"
        class="com.fdays.tsms.airticket.AirticketOrder"
        not-null="false"
    >

        <column name="air_order_id" />
    </many-to-one>
</class>

</hibernate-mapping>