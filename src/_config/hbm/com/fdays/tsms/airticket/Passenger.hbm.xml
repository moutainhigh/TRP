<?xml version="1.0" encoding="UTF-8"?>
<!-- [syncWithModel=true] -->
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping >

<class 
  name="com.fdays.tsms.airticket.Passenger"
	table="passenger"
>

<meta attribute="extends">org.apache.struts.action.ActionForm implements Cloneable</meta>
<meta attribute="class-code"><![CDATA[
  public Object clone()
  {
    Object o = null;
    try
    {
      o = super.clone();
    }
    catch (CloneNotSupportedException e)
    {
      e.printStackTrace();
    }
    return o;
}

private String thisAction="";
 public String getThisAction()
 {
     return thisAction;
 }


public void setThisAction(String thisAction)
 {
     this.thisAction=thisAction;
 }

private int index=0;
 public int getIndex()
 {
     return index;
 }


public void setIndex(int index)
 {
     this.index=index;
 }
 




]]></meta>


    <id
        name="id"
        type="long"
        column="id"
    >
        <generator class="sequence" >
          <param  name="sequence">seq_passenger</param>
          </generator>
    </id>

    <property
        name="name"
        column="name"
        type="string"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="ticketNumber"
        column="ticket_number"
        type="string"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="cardno"
        column="cardno"
        type="string"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="type"
        column="type"
        type="java.lang.Long"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="orderStatus"
        column="order_status"
        type="java.lang.Long"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="status"
        column="status"
        type="java.lang.Long"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="flightId"
        column="flight_id"
        type="java.lang.Long"
        not-null="false"
        insert="true" update="true"
    >
	</property>



    <many-to-one
        name="airticketOrder"
        class="com.fdays.tsms.airticket.AirticketOrder"
        not-null="false"
    >

        <column name="air_order_id" />
    </many-to-one>
</class>

</hibernate-mapping>