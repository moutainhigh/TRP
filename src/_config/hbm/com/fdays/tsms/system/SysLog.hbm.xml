<?xml version="1.0" encoding="UTF-8"?>
<!-- [syncWithModel=true] -->
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping >

<class 
  name="com.fdays.tsms.system.SysLog"
	table="sys_log"
>

<meta attribute="extends">org.apache.struts.action.ActionForm implements Cloneable</meta>
<meta attribute="class-code"><![CDATA[
  public Object clone()
  {
    Object o = null;
    try
    {
      o = super.clone();
    }
    catch (CloneNotSupportedException e)
    {
      e.printStackTrace();
    }
    return o;
}

private String thisAction="";
 public String getThisAction()
 {
     return thisAction;
 }


public void setThisAction(String thisAction)
 {
     this.thisAction=thisAction;
 }

private int index=0;
 public int getIndex()
 {
     return index;
 }


public void setIndex(int index)
 {
     this.index=index;
 }
 




]]></meta>


    <id
        name="id"
        type="long"
        column="id"
    >
        <generator class="sequence" >
          <param  name="sequence">seq_syslog</param>
          </generator>
    </id>

    <property
        name="logType"
        column="log_type"
        type="java.lang.Long"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="logDate"
        column="log_date"
        type="java.sql.Timestamp"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="logContent"
        column="log_content"
        type="string"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="locate"
        column="locate"
        type="java.lang.Long"
        not-null="false"
        insert="true" update="true"
    >
	</property>



  	<set
      name="logDetails"
      inverse="true"
  	  cascade="all-delete-orphan"
      >
          <key>
  		        <column name="log_id" />
          </key>
          <one-to-many 
              class="com.fdays.tsms.system.LogDetail"
          />
    </set>
    <many-to-one
        name="sysUser"
        class="com.fdays.tsms.user.SysUser"
        not-null="false"
    >

        <column name="user_id" />
    </many-to-one>
</class>

</hibernate-mapping>