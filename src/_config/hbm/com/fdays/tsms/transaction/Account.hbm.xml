<?xml version="1.0" encoding="UTF-8"?>
<!-- [syncWithModel=true] -->
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping >

<class 
  name="com.fdays.tsms.transaction.Account"
	table="account"
>

<meta attribute="extends">org.apache.struts.action.ActionForm implements Cloneable</meta>
<meta attribute="class-code"><![CDATA[
  public Object clone()
  {
    Object o = null;
    try
    {
      o = super.clone();
    }
    catch (CloneNotSupportedException e)
    {
      e.printStackTrace();
    }
    return o;
}

private String thisAction="";
 public String getThisAction()
 {
     return thisAction;
 }


public void setThisAction(String thisAction)
 {
     this.thisAction=thisAction;
 }

private int index=0;
 public int getIndex()
 {
     return index;
 }


public void setIndex(int index)
 {
     this.index=index;
 }
 




]]></meta>


    <id
        name="id"
        type="long"
        column="id"
    >
        <generator class="sequence" >
          <param  name="sequence">seq_account</param>
          </generator>
    </id>

    <property
        name="name"
        column="name"
        type="string"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="accountNo"
        column="account_no"
        type="string"
        not-null="false"
        insert="true" update="true"
    >
	</property>
	
	<!-- 实收款 -->
	  <property
     name="actualAmount" 
     type="java.math.BigDecimal" 
     formula="(select cal_actualAmount_by_accountId(id) from dual)"
      unique="false" 
      optimistic-lock="true" 
      lazy="false" 
    generated="never" >
    </property> 
    
    <!-- 未结款 -->
     <property
     name="unsettledAccount" 
     type="java.math.BigDecimal" 
     formula="(select cal_unsetamount_by_accountid(id) from dual)"
      unique="false" 
      optimistic-lock="true" 
      lazy="false" 
    generated="never" >
    </property> 
   
   <!-- 现返佣金 -->
     <property
     name="commission" 
     type="java.math.BigDecimal" 
     formula="(select cal_commission_by_accountid(id) from dual)"
      unique="false" 
      optimistic-lock="true" 
      lazy="false" 
    generated="never" >
    </property> 
    
    <!-- 后返佣金 -->
     <property
     name="rakeOff" 
     type="java.math.BigDecimal" 
     formula="(select cal_rakeoff_by_accountid(id) from dual)"
      unique="false" 
      optimistic-lock="true" 
      lazy="false" 
    generated="never" >
    
    
    </property> 
	
    <property
        name="type"
        column="type"
        type="java.lang.Long"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="status"
        column="status"
        type="java.lang.Long"
        not-null="false"
        insert="true" update="true"
    >
	</property>



    <many-to-one
        name="paymentTool"
        class="com.fdays.tsms.transaction.PaymentTool"
        not-null="false"
    >

        <column name="pay_tool_id" />
    </many-to-one>
  	<set
      name="platComAccounts"
      inverse="true"
  	  cascade="all-delete-orphan"
      >
          <key>
  		        <column name="account_id" />
          </key>
          <one-to-many 
              class="com.fdays.tsms.transaction.PlatComAccount"
          />
    </set>
</class>

</hibernate-mapping>