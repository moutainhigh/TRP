<?xml version="1.0" encoding="UTF-8"?>
<!-- [syncWithModel=true] -->
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping >

<class 
  name="com.fdays.tsms.transaction.Statement"
	table="statement"
>

<meta attribute="extends">org.apache.struts.action.ActionForm implements Cloneable</meta>
<meta attribute="class-code"><![CDATA[
  public Object clone()
  {
    Object o = null;
    try
    {
      o = super.clone();
    }
    catch (CloneNotSupportedException e)
    {
      e.printStackTrace();
    }
    return o;
}

private String thisAction="";
 public String getThisAction()
 {
     return thisAction;
 }


public void setThisAction(String thisAction)
 {
     this.thisAction=thisAction;
 }

private int index=0;
 public int getIndex()
 {
     return index;
 }


public void setIndex(int index)
 {
     this.index=index;
 }
 




]]></meta>


    <id
        name="id"
        type="long"
        column="id"
    >
        <generator class="sequence" >
          <param  name="sequence">seq_statement</param>
          </generator>
    </id>

    <property
        name="statementNo"
        column="statement_no"
        type="string"
        not-null="false"
        insert="true" update="true"
    >
	</property>
  <many-to-one name="fromPCAccount"
		class="com.fdays.tsms.transaction.PlatComAccount" not-null="false">
		<column name="from_account_id" />
		
	</many-to-one>
		<many-to-one name="toPCAccount"			class="com.fdays.tsms.transaction.PlatComAccount" not-null="false">
		<column name="to_account_id" />
	</many-to-one>

    <property
        name="orderType"
        column="order_type"
        type="java.lang.Long"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="totalAmount"
        column="total_amount"
        type="java.math.BigDecimal"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="actualAmount"
        column="actual_amount"
        type="java.math.BigDecimal"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="unsettledAccount"
        column="unsettled_account"
        type="java.math.BigDecimal"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="commission"
        column="commission"
        type="java.math.BigDecimal"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="rakeOff"
        column="rake_off"
        type="java.math.BigDecimal"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="optTime"
        column="opt_time"
        type="java.sql.Timestamp"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="type"
        column="type"
        type="java.lang.Long"
        not-null="false"
        insert="true" update="true"
    >
	</property>
    <property
        name="status"
        column="status"
        type="java.lang.Long"
        not-null="false"
        insert="true" update="true"
    >
	</property>



  	<set
      name="airticketOrders"
      inverse="true"
  	  cascade="all-delete-orphan"
      >
          <key>
  		        <column name="statement_id" />
          </key>
          <one-to-many 
              class="com.fdays.tsms.airticket.AirticketOrder"
          />
    </set>
    <many-to-one
        name="sysUser"
        class="com.fdays.tsms.user.SysUser"
        not-null="false"
    >

        <column name="user_id" />
    </many-to-one>
</class>

</hibernate-mapping>